config BOARD_HEALTHYPI_MOVE
	select SOC_NRF5340_CPUAPP_QKAA if BOARD_HEALTHYPI_MOVE_NRF5340_CPUAPP
	select SOC_NRF5340_CPUAPP_QKAA if BOARD_HEALTHYPI_MOVE_NRF5340_CPUAPP_NS
	select SOC_NRF5340_CPUNET_QKAA if BOARD_HEALTHYPI_MOVE_NRF5340_CPUNET